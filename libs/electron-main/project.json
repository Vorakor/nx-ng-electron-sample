{
  "root": "libs/electron-main",
  "sourceRoot": "libs/electron-main/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/electron-main",
        "tsConfig": "libs/electron-main/tsconfig.lib.json",
        "packageJson": "libs/electron-main/package.json",
        "main": "libs/electron-main/src/index.ts",
        "assets": ["libs/electron-main/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/electron-main/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/electron-main"],
      "options": {
        "jestConfig": "libs/electron-main/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
